   "source": [
    "#Re-defining for better interpretation\n",
    "def rename_predictions(row):\n",
    "    if row['Predictions'] == 0:\n",
    "        return 'No Disease'\n",
    "    else:\n",
    "        return \"Has Disease\"\n",
    "    \n",
    "    \n",
    "def rename_actual(row):\n",
    "    if row['Actual'] == 0:\n",
    "        return 'No Disease'\n",
    "    else:\n",
    "        return 'Has Disease'"
   ]
  },
