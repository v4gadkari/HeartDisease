    "#Set up models and steps for pipelines\n",
    "\n",
    "\n",
    "randForest = RandomForestClassifier()\n",
    "\n",
    "svc = LinearSVC()\n",
    "\n",
    "knn = KNeighborsClassifier()\n",
    "\n",
    "logReg = LogisticRegression()\n",
    "\n",
    "\n"
   ]
  },
  {
